            android:padding="10sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewEdadDonanteSupervivenciaInjerto"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewModeloSupervivenciaInjerto" />

