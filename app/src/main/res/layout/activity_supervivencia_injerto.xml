        <!-- Se añade texto al thumb, añadiendo el atributo app:showText="true". -->
        <!-- Se personaliza el texto del interruptor para los estados de "apagado" y -->
        <!-- "encendido" con los atributos android:textOn="" y android:textOff="" -->
